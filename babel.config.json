{
    "presets": [
        "@vue/babel-preset-jsx",
        [
            "@babel/preset-env",
            {
                "targets": {
                    "chrome": "67"
                },
                "useBuiltIns": "usage",
                "corejs": "3.18"
            }
        ],
        "@babel/preset-typescript"
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime",
            {
                "useESModules": true
            }
        ]
    ]
}