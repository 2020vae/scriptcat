{
    "manifest_version": 2,
    "name": "ScriptCat",
    "version": "0.0.1",
    "description": "",
    "background": {
        "scripts": [
            "src/background.js"
        ]
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": true
    },
    "permissions": [
        "tabs",
        "storage",
        "webRequest",
        "<all_urls>",
        "webRequestBlocking"
    ]
}